module ElasticGraph
  class GraphQL
    module Resolvers
      type fieldArgs = ::Hash[::Symbol, untyped]

      interface _Resolver
        def can_resolve?: (field: Schema::Field, object: untyped) -> bool
        def call: (
          ::GraphQL::Schema::_Type,
          ::GraphQL::Schema::Field,
          untyped,
          fieldArgs,
          ::GraphQL::Query::Context
        ) -> untyped
      end
    end
  end
end
